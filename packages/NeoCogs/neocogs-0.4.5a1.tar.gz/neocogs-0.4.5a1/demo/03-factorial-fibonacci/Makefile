
default:
	@echo -n '$$ '
	neocogs factorial 10
	@echo -n '$$ '
	neocogs fibonacci 10
	@echo -n '$$ '
	neocogs help
	@echo -n '$$ '
	neocogs help factorial
	@echo -n '$$ '
	neocogs help fibonacci
	@echo -n '$$ '
	-neocogs factorial ten
	@echo -n '$$ '
	-neocogs factorial -- -10
	@echo -n '$$ '
	-neocogs fibonacci ten
	@echo -n '$$ '
	-neocogs fibonacci -- -10

