digraph legend {
    rankdir=LR;
    graph [fontname = "helvetica"];
    node [fontname = "helvetica"];
    edge [fontname = "helvetica";];

    node [shape=point; height=0]

    A  -> B [label=<<I>   inherits from   </I>>;];

    B -> C [style=invis];
    C -> D [label=<<I>   delegates to   </I>>; style=dashed;];
}
